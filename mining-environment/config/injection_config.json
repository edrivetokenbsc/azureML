{
    "target_process": "python_app.py",
    "injection_script": "/app/scripts/injection_scripts/libcustomcode.so",
    "cloaking_endpoint": "https://cloaking.example.com/api/notify",
    "execution_schedule": {
      "frequency": "daily",
      "time": "02:00"
    },
    "security": {
      "access_control": {
        "allowed_users": ["injector_user"],
        "permissions": {
          "/app/scripts/injection_scripts/": "750"
        }
      }
    },
    "logging": {
      "log_level": "INFO",
      "log_format": "JSON",
      "log_file": "/app/logs/inject_code.log"
    },
    "resource_limits": {
      "cpu": "50%",
      "ram": "512MB"
    },
    "notification": {
      "email": {
        "enabled": false
      },
      "slack": {
        "enabled": false,
        "webhook_url": "https://hooks.slack.com/services/XXX/YYY/ZZZ"
      }
    },
    "retry_policy": {
      "max_retries": 3,
      "backoff_strategy": "exponential",
      "initial_delay_seconds": 10
    },
    "version_control": {
      "script_version": "1.0.0",
      "injection_module": "v2.1.3"
    }
  }
  